{
  "name": "dieti-estates-25-backend",
  "version": "1.0.0",
  "description": "Backend per DietiEstates25 - Piattaforma gestione servizi immobiliari",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run build:watch\" \"nodemon dist/index.js\"",
    "dev:auth": "cross-env SERVICE=auth nodemon --exec ts-node src/services/auth-service/index.ts",
    "dev:property": "cross-env SERVICE=property nodemon --exec ts-node src/services/property-service/index.ts",
    "dev:search": "cross-env SERVICE=search nodemon --exec ts-node src/services/search-service/index.ts",
    "dev:user": "cross-env SERVICE=user nodemon --exec ts-node src/services/user-service/index.ts",
    "dev:notification": "cross-env SERVICE=notification nodemon --exec ts-node src/services/notification-service/index.ts",
    "dev:all": "concurrently \"npm run dev:auth\" \"npm run dev:property\" \"npm run dev:search\" \"npm run dev:user\" \"npm run dev:notification\"",
    "create:agency": "ts-node scripts/create-agency.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.8",
    "@types/nodemailer": "^6.4.14",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@types/web-push": "^3.6.3",
    "@types/yamljs": "^0.2.34",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "oracledb": "^6.9.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.1",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "web-push": "^3.6.6",
    "winston": "^3.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jest": "^29.5.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.8.7",
    "@types/oracledb": "^6.9.1",
    "@types/supertest": "^2.0.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "real-estate",
    "microservices",
    "typescript",
    "express",
    "sequelize",
    "oracle",
    "jwt"
  ],
  "author": "DietiEstates25 Team",
  "license": "MIT"
}
